<h1>Listing orders</h1>

<table>
  <thead>
    <tr>
      <th>Appoint date</th>
      <th>Appoint time</th>
      <th>Status</th>
      <th>Total</th>
      <th>P deduct</th>
      <th>User</th>
      <th>Address</th>
      <th>Intro</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.appoint_date %></td>
        <td><%= order.appoint_time %></td>
        <td><%= order.status %></td>
        <td><%= order.total %></td>
        <td><%= order.p_deduct %></td>
        <td><%= order.user_id %></td>
        <td><%= order.address_id %></td>
        <td><%= order.intro %></td>
        <td><%= link_to 'Show', order %></td>
        <td><%= link_to 'Edit', edit_order_path(order) %></td>
        <td><%= link_to 'Destroy', order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order', new_order_path %>
